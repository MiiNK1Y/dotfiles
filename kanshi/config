output 'LG Electronics LG HDR WQHD+ 307NTAB8Y560' {
  mode 3840x1600
  position 0,0
  alias $HOME_LG
}

# Laptop monitor center beneath LG
output 'BOE 0x094C Unknown' {
  mode 1920x1200
  position 960,1600
  alias $INTERNAL
}

profile home_only_ultrawide {
  output $HOME_LG enable
  output $INTERNAL disable
}

profile only_laptop {
  output $INTERNAL enable
}
